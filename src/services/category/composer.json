{
    "name": "iagapie/category-service",
    "description": "Category Service App",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Agapie",
            "email": "igoragapie@gmail.com"
        }
    ],
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/iagapie/migrations"
    }],
    "require": {
        "php": ">= 8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "^3.1",
        "doctrine/migrations": "3.1.x-dev",
        "iagapie/php-mediator": "^0.0.1",
        "iagapie/php-micro": "^1.0",
        "monolog/monolog": "^2.2",
        "ramsey/uuid": "^4.1",
        "symfony/cache": "^5.3",
        "symfony/validator": "^5.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CategoryService\\": "src/"
        }
    }
}
