{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "ie": 11,
        "chrome": 52,
        "browsers": ["last 2 versions", "safari 9"]
      },
      "modules": false
    }],
    ["@babel/preset-react", {
      "runtime": "automatic"
    }]
  ],
  "plugins": ["@babel/plugin-proposal-class-properties", "babel-plugin-styled-components"]
}
